    #     # WebDriverWait(driver, 10).until(EC.element_to_be_clickable((By.XPATH, "/html/body/div[2]/div[2]/div[1]/div/div[2]/div[3]/div/div[13]/a[2]"))).click()
    #     safe_do(driver, lambda: WebDriverWait(driver, 10).until(
