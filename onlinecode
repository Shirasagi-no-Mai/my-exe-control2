    #         EC.element_to_be_clickable((By.XPATH, "//html/body/form/div[3]/div[2]/div[2]/div/div[2]/div/div[1]/div[1]/div[1]/span[2]/ul/li[1]/a"))    ).click(), "ç‚¹å‡»æŸä¸ªæŒ‰é’®")

